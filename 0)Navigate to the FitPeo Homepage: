import org.openqa.selenium.chrome.ChromeDriver;



public class OpenFitPeo {



	public static void main(String[] args) {

	

			System.setProperty("webdriver.chrome.driver", "C:\\Users\\Avi\\Downloads\\chromedriver-win64\\chromedriver.exe");

